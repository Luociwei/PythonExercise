"duts": {

        "dut0": {
            "xavier": {
                    "class": "Xavier_SC",
                    "log_folder": "/mix/addon/dut_firmware/ch1"
                 },
              "uart": {
                    "class":"UART",
                    "dev_name":"/dev/ttyS2"
                },

                "uart_dfu": {
                    "class": "UART2NET_UUT",
                    "dev_name": "/dev/ttyS2",
                    "port":7601
                },

                "uart_SoC":{
                    "class": "UartService",
                    "data_path_manager": "@datapath_manager",
                    "uart_service":{
                        "class": "UART",
                        "dev_name": "/dev/ttyS2",
                        "baudrate": 115200,
                        "data_bits": 8,
                        "parity": "none",
                        "stop_bits":1
                        }
                },


                "io_exp1": {
                    "class": "CAT9555",
                    "dev_addr": 32,
                    "i2c_bus": "@i2c_1"
                },
                "io_exp2": {
                    "class": "CAT9555",
                    "dev_addr": 33,
                    "i2c_bus": "@i2c_1"
                },
                "io_exp3": {
                    "class": "CAT9555",
                    "dev_addr": 34,
                    "i2c_bus": "@i2c_1"
                },

                "batt_dac": {
                    "class":"AD5602",
                    "dev_addr":15,
                    "i2c_bus":"@i2c_5",
                    "mvref":5000
                },

                "ltc2309": {
                    "class":"LTC2309",
                    "dev_addr":8,
                    "mode":"SINGLE_ENDED_UNIP",
                    "bus_rate":400000,
                    "i2c_bus":"@i2c_5"
                },
                "mcp47fe":{
                    "class":"MCP47FE",
                    "dev_addr":96,
                    "i2c_bus":"@i2c_5",
                    "ref_mode":"VREF_VDD"
                },

                "ocp_status": {
                    "class": "GPIO",
                    "default_dir": "input",
                    "pin_id":1000
                },

                "fan_pwm_output": {
                    "class": "MIXSignalSourceSG",
                    "axi4_bus":{
                        "class": "AXI4LiteBus",
                        "dev_name": "/dev/MIX_SignalSource_SG_0",
                        "reg_size": 1024
                    }
                },
                "fixture":{
                "class":"FixtureAction",
                "start_left_key":"@gpio_994",
                "start_right_key":"@gpio_996",
                "reset_key":"@gpio_995",
                "io_exp":    "@io_exp6"
                },
                "led":{
                  "class":"LED",
                  "ioexp":"@io_exp4",
                  "slot" :"slot1"
                },
                "site": 0,
                "server":{
                "log_folder_path": "/var/log/rpc_log",
                "endpoint": "tcp://*:7801",
                "log_level": "INFO"

                },
                "fwdl1":{
                       "class": "SmartGiantFWDL",
                       "slot_id":1,
                       "spi": "/dev/MIX_QSPI_SG_0",
                       "spi_freq":10000000,
                       "spi_mode":"MODE0"

                },
    },
    
