[run]
branch = True
omit = 
    */tests/*
    docs/*
    */debugger.py
    ./__init__.py
    levels.py
    profile_client.py
    publisher.py
    rpc_profile_python_client.py
    sample_code.py
    start_python_rpc_server.py
    artifact/*
    dependencies

[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover

    # Don't complain about missing debug-only code:
    def __repr__
    if self\.debug

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:

ignore_errors = True
    
[html]
directory = /Working/HWTEGit/SMT-TinyrpcX/htmlcov

